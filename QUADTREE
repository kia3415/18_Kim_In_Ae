#include<iostream>
#include<string>
using namespace std;

string recursive(string picture, int &index) {
	if (picture[index] != 'x') {
		string str = "";
		str += picture[index];
		return str;
	}
	index++;
	string s2 = recursive(picture, index);
	index++;
	string s1 = recursive(picture, index);
	index++;
	string s3 = recursive(picture, index);
	index++;
	string s4 = recursive(picture, index);

	string str = s3 + s4 + s2 + s1;
	return "x" + str;
}

int main() {

	int case_num; // 테스트 케이스 수
	cin >> case_num;
  
	string* picture = new string[case_num]; // n개의 그림정보
	for (int i = 0; i < case_num; i++)
		cin >> picture[i];

	for (int i = 0; i < case_num; i++) {
		int index = 0;
		string result = recursive(picture[i], index);
		cout << result << endl;
	}

	return 0;
}

